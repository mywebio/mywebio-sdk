package io.myweb;

public class NewActualParam {

	private final Class<?> clazz;

	private final Object val;

	public NewActualParam(Class<?> clazz, Object val) {
		this.clazz = clazz;
		this.val = val;
	}

	public Class<?> getClazz() {
		return clazz;
	}

	public Object getVal() {
		return val;
	}
}
